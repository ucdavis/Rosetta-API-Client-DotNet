    "/graphql": {
      "post": {
        "description": "GraphQL endpoint for IAM.\n\nIt supports queries on `Person` as defined in the GraphQL Schema under Documentation.\n\nExamples:\n\nSingle Data Source:\n      {\n          \"query\": \"{ people(limit: 10) { iam_id displayname email { primary } } }\"\n      }\n      \nMultiple Data Sources:\n      {\n      \"query\": \"{ \n          people( limit: 1 affiliationContains: \\\"student\\\" ) \n          { \n              iam_id        \n              name {lived_first_name lived_last_name}\n              id {login_id}\n              email {primary}\n              phone {primary}\n              student_association {college major academic_level class_level}\n          }\n          colleges\n          {\n              college_code\n              college_title\n          }\n          majors\n          {\n              major_title\n              major_code\n          }\n          }\"\n      }\n",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "x-amf-mediaType": "application/json",
            "in": "body",
            "name": "generated",
            "schema": {}
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "x-amf-mediaType": "application/json",
            "schema": {
              "description": "Standard GraphQL response",
              "example": {
                "data": {
                  "people": [
                    {
                      "iam_id": "1234567890",
                      "displayname": "Alice Example",
                      "email": [
                        {
                          "primary": "aexample@ucdavis.edu"
                        }
                      ]
                    }
                  ]
                }
              }
            }
          }
        },
        "security": [
          {
            "oauth_2_0": []
          }
        ]
      }
    },
