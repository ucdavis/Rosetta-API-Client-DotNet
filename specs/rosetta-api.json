  "basePath": "/api/{version}",
  "schemes": [
    "https"
  ],
  "externalDocs": {
    "url": "http://",
    "description": "  \n  type ProvisioningStatus {\n    primary: String\n  }\n\n  type NameRecord {\n    lived_first_name: String\n    lived_middle_name: String\n    lived_last_name: String\n    legal_first_name: String\n    legal_middle_name: String\n    legal_last_name: String\n  }\n\n  type IdentityIds {\n    iam_id: String\n    login_id: String\n    student_id: String\n    mothra_id: String\n    employee_id: String\n    mail_id: String\n    pidm: String\n    pps_id: String\n    cosmos_id: String\n    cpe_id: String\n    health_affiliate_id: String\n    ucanr_id: String\n    usda_whnrc_id: String\n    affiliate_id: String\n    ucnet_id: String\n  }\n\n  type EmailAddresses {\n    primary: String\n    work: String\n    personal: String\n  }\n\n  type PhoneNumbers {\n    primary: String\n    work: String\n    personal: String\n  }\n\n  type StudentAssociation {\n    college: String\n    major: String\n    academic_level: String\n    class_level: String\n  }\n\n  type PayrollAssociation {\n    employee_record: String\n    employee_id: String\n    position_number: String\n    position_title: String\n    relationship_to_organization: String\n    employee_classification: String\n    employee_classification_description: String\n    status: String\n    hire_date: String\n    start_date: String\n    termination_date: String\n    fte_percentage: String\n    reports_to_position: String\n    job_type_id: String\n    job_type_description: String\n    job_family_id: String\n    job_family_description: String\n    organization_id: String\n    organization_title: String\n    division_id: String\n    division_title: String\n    subdivision_id: String\n    subdivision_title: String\n    business_unit_id: String\n    business_unit_title: String\n    department_id: String\n    department_title: String\n    department_short_title: String\n  }\n\n  type Person {\n    iam_id: ID\n    displayname: String\n    birth_date: String\n    manager_iam_id: String\n    provisioning_status: [ProvisioningStatus]\n    affiliation: [String]\n    employment_status: [String]\n    name: [NameRecord]\n    id: [IdentityIds]\n    email: [EmailAddresses]\n    phone: [PhoneNumbers]\n    student_association: [StudentAssociation]\n    payroll_association: [PayrollAssociation]\n    modified_date: String\n    create_date: String\n  }\n\n  type College {\n    college_code: String\n    college_title: String\n  }\n\n  type Major {\n    major_code: String\n    major_title: String\n    major_status: String\n  }\n\n  type Query {\n    colleges(\n      college_code: String\n      college_title: String\n    ): [College]\n    majors(\n      major_code: String\n      major_title: String\n      major_status: String\n    ): [Major]\n    people(\n      iamid: String\n      manager_iam_id: String\n      iamids: [String]\n      email: String\n      emails: [String]\n      loginid: String\n      employeeid: String\n      studentid: String\n      mailid: String\n      pidm: String\n      mothraid: String\n      pps_id: String\n      cosmos_id: String\n      cpe_id: String\n      health_affiliate_id: String\n      ucanr_id: String\n      usda_whnrc_id: String\n      affiliate_id: String\n      ucnet_id: String\n      affiliationContains: String\n      employmentStatus: String\n      limit: Int = 10000\n    ): [Person]\n  }",
    "x-amf-title": "GraphQL Schema"
  },
